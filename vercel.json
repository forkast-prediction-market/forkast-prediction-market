{
  "functions": {
    "src/app/api/sync-events/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/migrate/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/setup-db/route.ts": {
      "maxDuration": 300
